<div class="row">
    <div class="col-md-12 table-responsive">
        <table class="table">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                </tr>
            </thead>
            <tbody>
                {{#each students}}
                <tr>
                    <td>{{this.firstName}}</td>
                    <td>{{this.lastName}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <a href="/students/add" class="btn btn-success float-right">Add New Student</a>
    </div>
</div>
